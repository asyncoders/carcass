{
  "name": "karcass",
  "version": "0.1.0",
  "description": "CLI tool for creating backend application's skeletons based on Express.js and TypeScript",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "build": "rm -Rf dist && tsc -p src",
    "watch": "rm -Rf dist && tsc -p src -w",
    "lint": "eslint -c .eslintrc.json --ext .js,.ts src/"
  },
  "files": [
    "index.js",
    "package.json",
    "README.md",
    "template",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asyncoders/karcass.git"
  },
  "keywords": [
    "skeleton",
    "express",
    "typescript",
    "typeorm",
    "oop",
    "backend"
  ],
  "author": "Valeriy Shibanov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asyncoders/karcass/issues"
  },
  "homepage": "https://github.com/asyncoders/karcass#readme",
  "dependencies": {
    "@karcass/cli": "0.0.3",
    "@karcass/template-reducer": "0.0.7",
    "inquirer": "^6.5.0"
  },
  "devDependencies": {
    "@types/inquirer": "^6.0.3",
    "@types/node": "^12.12.14",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.7.2",
    "eslint-plugin-react": "^7.14.3",
    "typescript": "^3.5.3"
  }
}
